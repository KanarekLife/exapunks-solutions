COPY 2 #AUTH
COPY 6 #AUTH

MARK TRACK_LOOP
COPY #TRAK T
REPL RECEIVER
LINK 801
GRAB T

MARK READER_LOOP
TEST EOF
TJMP FILE_END
COPY F T
MODE; SET MODE TO LOCAL
REPL TESTER
NOOP
TEST MRD
MODE; SET MODE TO GLOBAL
FJMP SEND_REPLACEMENET
KILL
SEEK -1
COPY F M
JUMP READER_LOOP

MARK SEND_REPLACEMENET
